    "@mui/material": "^5.11.1",
    "@react-pdf/renderer": "^3.1.9",
    "@testing-library/user-event": "^13.5.0",
    "@types/eslint": "^8.44.2",
    "@types/node": "^16.11.54",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/typescript": "^2.0.0",
    "axios": "^1.4.0",
    "electron-is-dev": "^2.0.0",
    "emailjs": "^4.0.2",
    "framer-motion": "^6.5.1",
    "js-cookie": "^3.0.5",
    "lx-react-form": "^1.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.8.9",
    "react-select": "^5.7.2",
    "react-signature-canvas": "^1.0.6",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.6",
    "sweetalert2": "^11.7.3",
    "sweetalert2-react-content": "^5.0.7",
    "typescript": "^4.7.4",
    "use-long-press": "^2.0.2",
    "usehooks-ts": "^2.9.0",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "start:web": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:mac": "npm run build:web && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build:web && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "start:desktop": "electron ./public/electron.js",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,tsx,ts}\"",
    "lint:full": "eslint \"src/**/*.{js,jsx,tsx,ts}\""
  },
  "build": {
    "appId": "com.exemple.electron",
    "productName": "Santa Imagem",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "public/logo_sem_background.ico"
    },
    "linux": {
      "icon": "public/logo_sem_background.png"
    },
    "mac": {
      "icon": "public/logo.icns"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react-signature-canvas": "^1.0.2",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "^20.1.0",
    "electron-builder": "^23.3.3",
    "eslint": "^8.47.0",
    "jest": "^29.5.0",
    "react-test-renderer": "^18.2.0",
    "serve": "^14.2.1",
    "ts-jest": "^29.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  }
}
